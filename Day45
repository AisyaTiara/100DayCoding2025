package aisya.thyara;

import java.util.Scanner;

public class Days_45 {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        int a = sc.nextInt();
        switch(a){
            case 1 : System.out.println("hari ke 1 senin");
            break;
            case 2 : System.out.println("hari ke 2 selasa"); 
            break;
            case 3 : System.out.println("hari ke 3 rabu"); 
            break;
            case 4 : System.out.println("hari ke 4 kamis ");
            break;
            case 5 : System.out.println("hari ke 5 jumat");
            break;
            case 6 : System.out.println("hari ke 6 sabtu");  
            break;
            case 7 : System.out.println("hari ke 7 minggu ");
            break;
            default:
                System.out.println("TIDAK VALID");
        }
        
        
        
    }
}
