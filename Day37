package pkg100haricoding;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
       Scanner sc = new Scanner (System.in);
       int nilai = sc.nextInt();
       if (nilai % 2 == 0){
            System.out.println("GENAP");
       }else{
            System.out.println("GANJIL");
    }
}}
