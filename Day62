import java.util.*;

public class aisya {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();

        int jumlah = 0;
        for (int i = 1; i <= N; i++) {
            System.out.println(i);
            jumlah += i;
        }
 
        System.out.println("Jumlah 1 sampai " + N + " adalah: " + jumlah);
    }
}
