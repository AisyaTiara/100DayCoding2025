public class Main {
	public static void main(String[] args) {
		String a = "Aisya";
		String b = "semangat belajar nya";
		String c ="";
		System.out.println(a.equals("aisya"));
		System.out.println(a.equalsIgnoreCase("aisya"));
		System.out.println(b.contains("nya"));
		System.out.println(b.isEmpty());
		System.out.println(c.isEmpty());
		
	}
}
