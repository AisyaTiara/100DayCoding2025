public class Main {
    public static void main(String[] args) {

        int[] angka = {12, 55, 3, 89, 40};  
        int min = angka[0];
        for (int i = 1; i < angka.length; i++) {
            if (angka[i] < min) {
                min = angka[i];  
            }
        }

        System.out.println("Nilai minimall dalam array adalah: " + min);
    }
}
